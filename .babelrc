{
    "presets": [
        "es2015-loose",
        "stage-0",
        "react"
    ],

    "env": {
        "development": {
            "plugins": [
                "transform-es3-member-expression-literals",
                "transform-es3-property-literals",
                "react-hot-loader/babel"
            ]
        },

        "production": {
            "plugins": [
                  "transform-es3-member-expression-literals",
                  "transform-es3-property-literals"
                ]
            }
    }
}
